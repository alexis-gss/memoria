        "overlayscrollbars-vue": "^0.5.9",
        "postcss-discard-comments": "^7.0.1",
        "prettier": "^3.3.2",
        "resumablejs": "^1.1.0",
        "rollup-plugin-copy": "^3.5.0",
        "sass": "^1.77.6",
        "slugify": "^1.6.6",
        "stylelint": "^16.6.1",
        "stylelint-config-idiomatic-order": "github:ream88/stylelint-config-idiomatic-order",
        "stylelint-config-sass-guidelines": "^11.1.0",
